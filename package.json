{
  "name": "ngx-form-helper",
  "version": "1.0.0",
  "description": "angular form validation helper",
  "scripts": {
    "build": "ng-packagr -p package.json && npm run build-style",
    "build-style": "webpack --config config/webpack.js && rimraf dist/ngx-form-helper.js",
    "publish": "npm publish dist",
    "clean": "rimraf dist",
    "demo": "webpack-dev-server --config config/webpack.demo.js --progress --open --profile --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@tweenjs/tween.js",
      "@types/tween.js",
      "@types/jquery",
      "jquery"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/zw277856645/ngx-form-helper"
  },
  "keywords": [],
  "author": "wei.zhang",
  "license": "MIT",
  "dependencies": {
    "@tweenjs/tween.js": "17.2.0",
    "@types/jquery": "2.0.43",
    "@types/tween.js": "16.9.0",
    "jquery": "3.2.1"
  },
  "devDependencies": {
    "@angular/animations": "7.2.14",
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/compiler-cli": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/forms": "7.2.14",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@angular/router": "7.2.14",
    "@types/node": "10.14.5",
    "animate.css": "3.6.1",
    "html-webpack-plugin": "2.16.1",
    "cmjs-lib": "0.2.7",
    "codelyzer": "4.5.0",
    "core-js": "2.4.1",
    "ng-packagr": "5.1.0",
    "rimraf": "2.6.1",
    "rxjs": "6.5.1",
    "rxjs-tslint": "0.1.7",
    "semantic-ui-css": "2.3.2",
    "html-loader": "0.4.3",
    "tslint": "5.11.0",
    "typescript": "3.2.4",
    "base-href-webpack-plugin": "1.0.2",
    "awesome-typescript-loader": "3.4.1",
    "angular2-template-loader": "0.6.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "zone.js": "0.8.29",
    "autoprefixer": "7.0.1",
    "css-loader": "0.26.1",
    "postcss-loader": "2.0.5",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.9.0",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5"
  }
}
